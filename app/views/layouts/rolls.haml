!!! 1.1
%html
  %head
    =stylesheet_link_tag 'main'
    = javascript_include_tag 'jquery', 'jquery.caret', 'dice_roll'
    %title Rolls 
  %body
    .logobox
      %img{:src => image_path('qrpsd.png')}
    .wrap
      .header
        =navh [['Home', root_path], ['About', about_path]]
        =loginh 
      .content
        - if flash[:warning] || flash[:notice] || flash[:error]
          #flash
            -flash.each do |name,msg|
              =msg
        =yield
