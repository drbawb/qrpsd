<%= error_messages_for :sheet %>

<% form_for(:sheet, :url => sheets_path, 
                      :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="sheet">Upload A Sheet:</label>
    <%= f.file_field :sheet %>
  </p>
  <p>
  	<%=collection_select(:sheet, :character_id, current_user.characters.all, :id, :name, {:prompt => true}) %>
  	<br />
  </p>
  <p>
    <%= submit_tag 'Create' %>
  </p>
<% end -%>
